# Analytics Webhook Configuration
# Copy this file to .env.local and fill in your values

# Webhook secret for /api/analytics/update endpoint
# Generate a random string: openssl rand -hex 32
ANALYTICS_WEBHOOK_SECRET=your-webhook-secret-here

# Aliyun API Key (for future direct API integration)
ALIYUN_API_KEY=sk-your-aliyun-api-key-here

# OpenClaw Gateway URL (optional, for system health checks)
OPENCLAW_GATEWAY_URL=https://your-gateway.zeabur.app
